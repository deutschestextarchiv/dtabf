        <body>
            <p>This is the TEI Customization for the DTA 'base format', used by the DFG Project
                Deutsches Textarchiv (The German Text Archive).</p>
            <schemaSpec ident="basisformat" docLang="en" prefix="tei_" start="TEI" xml:lang="en">
                <!-- required modules -->
                <moduleRef key="core" include="abbr add addrLine address author bibl biblScope cb choice cit corr date del editor email expan foreign gap head hi item l lb lg list listBibl measure milestone name note orig p pb pubPlace publisher q quote ref reg resp respStmt sic sp speaker stage title unclear"/>
                <moduleRef key="header" include="abstract availability biblFull classCode edition editionStmt editorialDecl encodingDesc extent fileDesc handNote idno langUsage language licence notesStmt profileDesc publicationStmt rendition seriesStmt sourceDesc tagsDecl teiHeader textClass titleStmt"/>
                <moduleRef key="tei"/>
                <moduleRef key="textstructure" include="TEI argument back body byline closer dateline div docAuthor docDate docEdition docImprint docTitle epigraph floatingText front imprimatur opener postscript salute signed text titlePage titlePart trailer"/>
                <!-- optional modules -->
                <moduleRef key="corpus" include="channel textDesc"/>
                <moduleRef key="drama" include="actor castGroup castItem castList role roleDesc spGrp"/>
                <moduleRef key="figures" include="cell figure formula row table"/>
                <moduleRef key="linking" include=""/><!-- @include remains empty because we only want to select attributes from that class (not elements) -->
                <moduleRef key="msdescription" include="handDesc msDesc msIdentifier physDesc repository typeDesc"/>
                <moduleRef key="namesdates" include="addName country forename genName nameLink orgName persName placeName roleName surname"/>
                <moduleRef key="textcrit" include=""/><!-- @include remains empty because we only want to select attributes from that class (not elements) -->
                <moduleRef key="transcr" include="fw metamark space subst supplied"/>
                
                <classSpec ident="att.breaking" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.canonical" module="tei" mode="change" type="atts">
                    <!-- includes @ref -->
                    <attList>
                        <attDef ident="key" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.coordinated" module="transcr" mode="delete" type="atts"/>
                <classSpec ident="att.damaged" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.datable" module="tei" mode="change" type="atts">
                    <!-- includes att.datable.w3c -->
                    <attList>
                        <attDef ident="calendar" mode="delete"/>
                        <attDef ident="period" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.datable.custom" module="namesdates" mode="delete" type="atts"/>
                <classSpec ident="att.datable.iso" module="namesdates" mode="delete" type="atts"/>
                <classSpec ident="att.datable.w3c" module="tei" mode="change" type="atts">
                    <!-- includes @when -->
                    <attList>
                        <attDef ident="notBefore" mode="delete"/>
                        <attDef ident="notAfter" mode="delete"/>
                        <attDef ident="from" mode="delete"/>
                        <attDef ident="to" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.datcat" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.declarable" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.declaring" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.dimensions" mode="change" type="atts" module="tei">
                    <!-- includes @unit, @quantity -->
                    <attList>
                        <attDef ident="precision" mode="delete"/>
                        <attDef ident="scope" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.divLike" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.docStatus" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.duration.iso" module="namesdates" mode="delete" type="atts"/>
                <classSpec ident="att.duration.w3c" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.edition" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.editLike" module="tei" mode="change" type="atts">
                    <!-- includes att.dimensions -->
                    <attList>
                        <attDef ident="evidence" mode="delete"/>
                        <attDef ident="instant" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.fragmentable" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.global" mode="change" type="atts" module="tei">
                    <!-- includes @xml:id, @n, @xml:lang, and other classes -->
                    <attList>
                        <attDef ident="xml:base" mode="delete"/>
                        <attDef ident="xml:space" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.global.change" module="transcr" mode="delete" type="atts"/>
                <classSpec ident="att.global.linking" mode="change" type="atts" module="linking">
                    <!-- includes @corresp, @next, @prev, @sameAs -->
                    <attList>
                        <attDef ident="synch" mode="delete"/>
                        <attDef ident="copyOf" mode="delete"/>
                        <attDef ident="exclude" mode="delete"/>
                        <attDef ident="select" mode="delete"/>
                        <attDef ident="corresp" mode="change">
                            <datatype minOccurs="1" maxOccurs="unbounded">
                                <rng:ref name="teidata.pointer"/>
                            </datatype>
                        </attDef>
                        <attDef ident="next" mode="change">
                            <datatype minOccurs="1" maxOccurs="unbounded">
                                <rng:ref name="teidata.pointer"/>
                            </datatype>
                        </attDef>
                        <attDef ident="prev" mode="change">
                            <datatype minOccurs="1" maxOccurs="unbounded">
                                <rng:ref name="teidata.pointer"/>
                            </datatype>
                        </attDef>
                        <attDef ident="sameAs" mode="change">
                            <datatype minOccurs="1" maxOccurs="unbounded">
                                <rng:ref name="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                </classSpec>
                <classSpec ident="att.global.rendition" module="tei" mode="change" type="atts">
                    <!-- includes @rend, @rendition -->
                    <attList>
                        <attDef ident="style" mode="delete"/>
                        <attDef ident="rendition" mode="change" usage="opt">
                            <datatype minOccurs="1" maxOccurs="unbounded">
                                <rng:ref name="teidata.pointer"/>
                            </datatype>
                            <valList type="closed" mode="replace">
                                <valItem ident="#topBraced"><desc>horizontale Klammerung oben</desc></valItem>
                                <valItem ident="#bottomBraced"><desc>horizontale Klammerung unten</desc></valItem>
                                <valItem ident="#leftBraced"><desc>vertikale Klammerung links</desc></valItem>
                                <valItem ident="#rightBraced"><desc>vertikale Klammerung rechts</desc></valItem>
                                <valItem ident="#fr"><desc>Frakturwechsel</desc></valItem>
                                <valItem ident="#aq"><desc>Antiqua-Schrift in Fraktur-Werken</desc></valItem>
                                <valItem ident="#right"><desc>rechtsbündig</desc></valItem>
                                <valItem ident="#c"><desc>zentriert</desc></valItem>
                                <valItem ident="#et"><desc>eingerückter Text</desc></valItem>
                                <valItem ident="#b"><desc>fett</desc></valItem>
                                <valItem ident="#g"><desc>gesperrt</desc></valItem>
                                <valItem ident="#k"><desc>Kapitälchen</desc></valItem>
                                <valItem ident="#i"><desc>kursiv</desc></valItem>
                                <valItem ident="#in"><desc>Initiale</desc></valItem>
                                <valItem ident="#u"><desc>einfach unterstrichen</desc></valItem>
                                <valItem ident="#uu"><desc>doppelt unterstrichen</desc></valItem>
                                <valItem ident="#sup"><desc>hochgestellt</desc></valItem>
                                <valItem ident="#sub"><desc>tiefgestellt</desc></valItem>
                                <valItem ident="#red"><desc>rot</desc></valItem>
                                <valItem ident="#blue"><desc>blau</desc></valItem>
                                <valItem ident="#s"><desc>durchgestrichen</desc></valItem>
                                <valItem ident="#v"><desc>vertikal gedruckt</desc></valItem>
                                <valItem ident="#f"><desc>Frakturschrift in Antiqua-Werken</desc></valItem>
                                <valItem ident="#none"><desc>ohne Wert; hier ist zusätzlich @rend zu verwenden</desc></valItem>
                                <!-- Level 3: -->
                                <valItem ident="#et2"><desc>(Level 3:) zweifach eingerückter Text</desc></valItem>
                                <valItem ident="#et3"><desc>(Level 3:) dreifach eingerückter Text</desc></valItem>
                                <valItem ident="#smaller"><desc>(Level 3:) gegenüber dem Grundtext verkleinerte Schrift</desc></valItem>
                                <valItem ident="#larger"><desc>(Level 3:) gegenüber dem Grundtext vergrößerte Schrift</desc></valItem>
                                <!-- Manuskripte: -->
                                <valItem ident="#mPrint"><desc>(Manuskripte:) Vorgedrucktes auf der Manuskriptseite</desc></valItem>
                                <valItem ident="#mRetrace"><desc>(Manuskripte:) nachgeschrieben</desc></valItem>
                            </valList>
                        </attDef>
                    </attList>
                </classSpec>
                <classSpec ident="att.global.responsibility" module="tei" mode="change" type="atts">
                    <!-- includes @cert, @resp -->
                    <attList>
                        <attDef ident="cert" mode="change" usage="opt">
                            <valList type="closed" mode="replace">
                                <valItem ident="high"/>
                                <valItem ident="low"/>
                            </valList>
                        </attDef>
                    </attList>
                </classSpec>
                <classSpec ident="att.global.source" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.handFeatures" module="tei" mode="change" type="atts">
                    <attList>
                        <attDef ident="script" mode="delete"/>
                        <attDef ident="scriptRef" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.internetMedia" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.interpLike" mode="delete" type="atts" module="tei"/>
                <classSpec ident="att.measurement" mode="delete" type="atts" module="tei"/>
                <classSpec ident="att.msExcerpt" module="msdescription" mode="delete" type="atts"/>
                <classSpec ident="att.naming" mode="change" type="atts" module="tei">
                    <!-- includes @role, att.canonical -->
                    <attList>
                        <attDef ident="nymRef" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.personal" module="tei" mode="change" type="atts">
                    <!-- includes @full -->
                    <attList>
                        <attDef ident="full" mode="replace" usage="opt">
                            <valList mode="replace" type="closed">
                                <valItem ident="abb"><desc>unvollständiger Teil eines Namens</desc></valItem>
                            </valList>
                        </attDef>
                        <attDef ident="sort" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.pointing" mode="change" type="atts" module="tei">
                    <!-- includes @target -->
                    <attList>
                        <attDef ident="evaluate" mode="delete"/>
                        <attDef ident="targetLang" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.pointing.group" module="tei" mode="change" type="atts">
                    <!-- includes att.pointing, att.typed -->
                    <attList>
                        <attDef ident="domains" mode="delete"/>
                        <attDef ident="targFunc" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.ranging" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.readFrom" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.scoping" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.segLike" module="tei" mode="change" type="atts"/>
                <classSpec ident="att.sortable" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.source" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.spanning" module="tei" mode="delete" type="atts"/>
            		<classSpec ident="att.textCritical" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.timed" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.transcriptional" module="tei" mode="change" type="atts">
                    <!-- includes att.written, att.dimensions and other classes -->
                    <attList>
                        <attDef ident="status" mode="delete"/>
                        <attDef ident="cause" mode="delete"/>
                        <attDef ident="seq" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.translatable" module="tei" mode="delete" type="atts"/>
                <classSpec ident="att.typed" mode="change" type="atts" module="tei">
                    <!-- includes @type -->
                    <attList>
                        <attDef ident="subtype" mode="delete"/>
                    </attList>
                </classSpec>
                
                
                <elementSpec ident="abbr" module="core" mode="change">
                   <attList>
                       <attDef ident="type" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                    </attList>
               </elementSpec>
               <elementSpec ident="actor" module="drama" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="add" module="core" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="xml:lang" mode="delete"/>
                       <attDef ident="place" mode="change" usage="req">
                           <valList type="closed" mode="replace">
                               <valItem ident="across"><desc>Manuskripte: Hinzufügung durch Überschreibung des zu korrigierenden Textes.</desc></valItem>
                               <valItem ident="intralinear"><desc>Manuskripte: Einfügung innerhalb der Textzeile, jedoch ohne bestehenden Text zu überschreiben.</desc></valItem>
                               <valItem ident="superlinear"><desc>Manuskripte: Einfügung über der Zeile.</desc></valItem>
                               <valItem ident="sublinear"><desc>Manuskripte: Einfügung unter der Zeile.</desc></valItem>
                               <valItem ident="left"><desc>Manuskripte: Einfügung am linken Rand.</desc></valItem>
                               <valItem ident="right"><desc>Manuskripte: Einfügung am rechten Rand.</desc></valItem>
                           </valList>
                       </attDef>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="unit" mode="delete"/>
                       <attDef ident="quantity" mode="delete"/>
                       <attDef ident="extent" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="type" mode="delete"/>
                       <attDef ident="subtype" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="argument" module="textstructure" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="back" module="textstructure" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="xml:lang" mode="delete"/>
                       <attDef ident="rendition" mode="delete"/>
                       <attDef ident="decls" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="rend" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="bibl" module="core" mode="change">
                   <attList>
                       <attDef ident="default" mode="delete"/>
                       <attDef ident="type" mode="change">
                           <valList type="closed" mode="replace">
                               <valItem ident="M"><desc>Monographie</desc></valItem>
                               <valItem ident="MM"><desc>Teil einer mehrbändigen Monographie</desc></valItem>
                               <valItem ident="DM"><desc>unselbständiger Teil einer Monographie</desc></valItem>
                               <valItem ident="DS"><desc>unselbständige Schrift in einem Band, der Teil einer Reihe ist</desc></valItem>
                               <valItem ident="JA"><desc>Artikel einer Zeitschrift/Zeitung</desc></valItem>
                               <valItem ident="J"><desc>Zeitschrift/Zeitung</desc></valItem>
                               <valItem ident="MMS"><desc>Teil einer mehrbändigen Monographie, die ihrerseits Teil einer Reihe ist</desc></valItem>
                               <valItem ident="MS"><desc>selbständiger Band einer Reihe</desc></valItem>
                               <valItem ident="MAN"><desc>handschriftliche Quelle/Manuskript</desc></valItem>
                           </valList>
                       </attDef>
                       <attDef ident="subtype" mode="delete"/>
                       <attDef ident="sortKey" mode="delete"/>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                    </attList>
               </elementSpec>
               <elementSpec ident="body" module="textstructure" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="xml:lang" mode="delete"/>
                       <attDef ident="rendition" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="rend" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="byline" module="textstructure" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="castGroup" module="drama" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="castItem" module="drama" mode="change">
                   <attList>
                       <attDef ident="type" mode="delete"/>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="castList" module="drama" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="cb" module="core" mode="change">
                   <attList>
                       <attDef ident="subtype" mode="delete"/>
                       <attDef ident="ed" mode="delete"/>
                       <attDef ident="spanTo" mode="delete"/>
                       <attDef ident="break" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="type" mode="change">
                           <valList type="closed" mode="replace">
                               <valItem ident="start"><desc>Beginn des Spaltensatzes</desc></valItem>
                               <valItem ident="end"><desc>Ende des Spaltensatzes</desc></valItem>
                           </valList>
                       </attDef>
                       <attDef ident="xml:lang" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="rendition" mode="delete"/>
                       <attDef ident="rend" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="cell" module="figures" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="role" mode="replace" usage="opt">
                          <valList type="closed" mode="replace">
                              <valItem ident="label"><desc>Spaltentitel</desc></valItem>
                          </valList>
                       </attDef>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="rows" mode="change">
                           <datatype minOccurs="1" maxOccurs="1">
                               <rng:ref name="teidata.count"/>
                           </datatype>
                       </attDef>
                       <attDef ident="cols" mode="change">
                           <datatype minOccurs="1" maxOccurs="1">
                               <rng:ref name="teidata.count"/>
                           </datatype>
                       </attDef>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="choice" module="core" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                       <attDef ident="rend" mode="delete"/>
                       <attDef ident="rendition" mode="delete"/>
                       <attDef ident="xml:lang" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                   </attList>
               </elementSpec>
               <elementSpec ident="cit" module="core" mode="change">
                   <attList>
                       <attDef ident="type" mode="delete"/>
                       <attDef ident="subtype" mode="delete"/>
                       <attDef ident="n" mode="delete"/>
                       <attDef ident="facs" mode="delete"/>
                       <attDef ident="sameAs" mode="delete"/>
                       <attDef ident="cert" mode="delete"/>
                       <attDef ident="resp" mode="delete"/>
                    </attList>
               </elementSpec>
               <elementSpec ident="closer" module="textstructure" mode="change">
                   <attList>
                       <attDef ident="n" mode="delete"/>
