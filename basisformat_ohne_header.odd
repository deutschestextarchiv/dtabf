        <body>
            <p>This is a TEI Customization for the DFG Project Deutsches Textarchiv (The German Text
                Archive), which does not consider the extensive usage of the TEI-Header.</p>
            <schemaSpec ident="basisformat_ohne_header" docLang="en" prefix="tei_" start="TEI"
                xml:lang="en">
                
                <moduleRef key="core"
                    except="addrLine address analytic biblScope biblStruct binaryObject citedRange desc distinct divGen editor email emph gb gloss graphic headItem headLabel imprint index label measure measureGrp media meeting mentioned monogr num postBox postCode ptr relatedItem respStmt resp rs said series soCalled street teiCorpus term textLang time"/>
                <moduleRef key="tei"/>
                
                <moduleRef key="header" include="abstract teiHeader fileDesc titleStmt publicationStmt sourceDesc"/>
                <moduleRef key="textstructure" except="div1 div2 div3 div4 div5 div6 div7 group"/>
                
                <moduleRef key="analysis" except="c cl interp interpGrp m pc phr s span spanGrp w"/>
                <moduleRef key="drama"
                    except="camera caption epilogue move performance prologue set sound tech view"/>
                <moduleRef key="figures" except="figDesc notatedMusic"/>
                <moduleRef key="transcr"
                    except="addSpan am damage damageSpan delSpan ex facsimile handNotes handShift line listTranspose mod redo restore retrace sourceDoc substJoin surface surfaceGrp surplus transpose undo zone"/>
                <moduleRef key="linking"
                    except="ab alt altGrp anchor join joinGrp link linkGrp seg timeline when"/>
                <moduleRef key="namesdates"
                    except="addName affiliation age birth bloc climate country death district education event faith floruit forename genName geo geogFeat geogName langKnowledge langKnown listEvent listNym listOrg listPerson listPlace listRelation location nameLink nationality nym occupation offset org person personGrp place population region relation relationGrp residence roleName settlement sex socecStatus state surname terrain trait"/>
                <elementSpec ident="fw" module="transcr" mode="change">
                    <attList>
                        <attDef ident="type" mode="change" usage="req">
                            <desc xml:lang="de"><att>n</att> gibt an, welcher Art das forme work
                                ist.</desc>
                            <datatype>
                                <rng:text/>
                            </datatype>
                            <valList type="closed" mode="replace">
                                <valItem ident="header"><desc>Kolumnentitel</desc></valItem>
                                <valItem ident="sig"><desc>Bogensignatur</desc></valItem>
                                <valItem ident="catch"><desc>Kustode</desc></valItem>
                                <valItem ident="pageNum"><desc>Seitennummer</desc></valItem>
                                <valItem ident="folNum"><desc>Blattnummer</desc></valItem>
                            </valList>
                        </attDef>
                        <attDef ident="place" mode="change" usage="req">
                            <desc>describes the place, where the forme work is found on the
                                page</desc>
                            <valList type="closed" mode="replace">
                                <valItem ident="top"><desc>am Seitenanfang gedruckt</desc></valItem>
                                <valItem ident="bottom"><desc>am Seitenende gedruckt</desc></valItem>
                            </valList>
                        </attDef>
                        <attDef ident="n" mode="delete"/>
                        <attDef ident="facs" mode="delete"/>
                        <attDef ident="sameAs" mode="delete"/>
                        
                    </attList>
                </elementSpec>
                
